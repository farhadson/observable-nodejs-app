{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": { "type": "grafana", "uid": "-- Grafana --" },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "description": "Traces (Tempo) + RED metrics from Tempo metrics-generator stored in Prometheus.",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "id": 101,
      "type": "stat",
      "title": "Service RPS (spanmetrics)",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${ds_prom}" },
      "pluginVersion": "11.5.2",
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(traces_spanmetrics_calls_total{service=~\"$service\"}[$__rate_interval]))",
          "legendFormat": "rps"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "orientation": "horizontal", "textMode": "auto", "colorMode": "value" },
      "fieldConfig": {
        "defaults": {
          "unit": "req/s",
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "red", "value": 100 }] }
        },
        "overrides": []
      }
    },
    {
      "id": 102,
      "type": "stat",
      "title": "Error % (status_code=ERROR)",
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${ds_prom}" },
      "pluginVersion": "11.5.2",
      "targets": [
        {
          "refId": "A",
          "expr": "100 * (sum(rate(traces_spanmetrics_calls_total{service=~\"$service\", status_code=\"STATUS_CODE_ERROR\"}[$__rate_interval])) / clamp_min(sum(rate(traces_spanmetrics_calls_total{service=~\"$service\"}[$__rate_interval])), 0.000001))",
          "legendFormat": "error%"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "orientation": "horizontal", "textMode": "auto", "colorMode": "value" },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 1 }, { "color": "red", "value": 5 }] }
        },
        "overrides": []
      }
    },
    {
      "id": 103,
      "type": "stat",
      "title": "p95 Latency (service)",
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${ds_prom}" },
      "pluginVersion": "11.5.2",
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum(rate(traces_spanmetrics_latency_bucket{service=~\"$service\"}[$__rate_interval])) by (le))",
          "legendFormat": "p95"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "orientation": "horizontal", "textMode": "auto", "colorMode": "value" },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 0.5 }, { "color": "red", "value": 1.5 }] }
        },
        "overrides": []
      }
    },
    {
      "id": 104,
      "type": "stat",
      "title": "Tempo metrics-generator up",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${ds_prom}" },
      "pluginVersion": "11.5.2",
      "targets": [
        {
          "refId": "A",
          "expr": "max(up{job=~\"tempo-metrics-generator\"})",
          "legendFormat": "up"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "orientation": "horizontal", "textMode": "auto", "colorMode": "value" },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "mappings": [
            { "type": "value", "options": { "0": { "text": "DOWN" }, "1": { "text": "UP" } } }
          ],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "red", "value": null }, { "color": "green", "value": 1 }] }
        },
        "overrides": []
      }
    },
    {
      "id": 105,
      "type": "timeseries",
      "title": "RPS by span_name (top series)",
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 4 },
      "datasource": { "type": "prometheus", "uid": "${ds_prom}" },
      "pluginVersion": "11.5.2",
      "targets": [
        {
          "refId": "A",
          "expr": "topk(10, sum(rate(traces_spanmetrics_calls_total{service=~\"$service\"}[$__rate_interval])) by (span_name))",
          "legendFormat": "{{span_name}}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "req/s" }, "overrides": [] },
      "options": { "legend": { "showLegend": true, "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "single" } }
    },
    {
      "id": 106,
      "type": "timeseries",
      "title": "p95 latency by span_name (top series)",
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 4 },
      "datasource": { "type": "prometheus", "uid": "${ds_prom}" },
      "pluginVersion": "11.5.2",
      "targets": [
        {
          "refId": "A",
          "expr": "topk(10, histogram_quantile(0.95, sum(rate(traces_spanmetrics_latency_bucket{service=~\"$service\"}[$__rate_interval])) by (le, span_name)))",
          "legendFormat": "{{span_name}}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "options": { "legend": { "showLegend": true, "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "single" } }
    },
    {
      "id": 107,
      "type": "heatmap",
      "title": "Latency histogram (all spans in service)",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 11 },
      "datasource": { "type": "prometheus", "uid": "${ds_prom}" },
      "pluginVersion": "11.5.2",
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(traces_spanmetrics_latency_bucket{service=~\"$service\"}[$__rate_interval])) by (le)",
          "legendFormat": "{{le}}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "options": { "calculate": false, "cellGap": 1, "color": { "mode": "scheme" }, "exemplars": { "color": "rgba(255,0,0,0.7)" }, "filterValues": { "le": 1e9 }, "legend": { "show": true }, "tooltip": { "show": true, "yHistogram": true } }
    },
    {
      "id": 8,
      "datasource": { "type": "tempo", "uid": "${ds_tempo}" },
      "fieldConfig": {
        "defaults": {
          "custom": { "align": "auto", "cellOptions": { "type": "auto" }, "inspect": false }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Trace ID" },
            "properties": [
              {
                "id": "links",
                "value": [
                  {
                    "title": "Set traceId",
                    "url": "/d/${__dashboard.uid}/${__dashboard.uid}?var-traceId=${__value.raw}",
                    "targetBlank": false
                  }
                ]
              }
            ]
          }
        ]
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 19 },
      "id": 8,
      "options": {
        "cellHeight": "sm",
        "footer": { "countRows": false, "fields": "", "reducer": ["sum"], "show": false },
        "showHeader": true,
        "sortBy": [{ "desc": true, "displayName": "Start time" }]
      },
      "pluginVersion": "11.5.2",
      "targets": [
        {
          "filters": [{ "id": "e8e0f06e", "operator": "=", "scope": "span" }],
          "hide": false,
          "limit": 200,
          "query": "$traceql",
          "queryType": "traceql",
          "refId": "A",
          "spss": 3,
          "tableType": "spans"
        }
      ],
      "title": "Trace List (Spans format)",
      "type": "table"
    },
    {
      "id": 7,
      "datasource": { "type": "tempo", "uid": "${ds_tempo}" },
      "fieldConfig": {
        "defaults": {
          "custom": { "align": "auto", "cellOptions": { "type": "auto" }, "inspect": false }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Trace ID" },
            "properties": [
              {
                "id": "links",
                "value": [
                  {
                    "title": "Set traceId",
                    "url": "/d/${__dashboard.uid}/${__dashboard.uid}?var-traceId=${__value.raw}",
                    "targetBlank": false
                  }
                ]
              }
            ]
          }
        ]
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 19 },
      "options": {
        "cellHeight": "sm",
        "footer": { "countRows": false, "fields": "", "reducer": ["sum"], "show": false },
        "showHeader": true,
        "sortBy": [{ "desc": true, "displayName": "Start time" }]
      },
      "pluginVersion": "11.5.2",
      "targets": [
        {
          "filters": [{ "id": "e8e0f06e", "operator": "=", "scope": "span" }],
          "hide": false,
          "limit": 1000,
          "query": "$traceql",
          "queryType": "traceql",
          "refId": "A",
          "spss": 3,
          "tableType": "traces"
        }
      ],
      "title": "Trace List (Trace format)",
      "type": "table"
    },
    {
      "id": 2,
      "datasource": { "type": "tempo", "uid": "${ds_tempo}" },
      "fieldConfig": { "defaults": {}, "overrides": [] },
      "gridPos": { "h": 12, "w": 24, "x": 0, "y": 27 },
      "options": {
        "spanFilters": {
          "criticalPathOnly": false,
          "matchesOnly": false,
          "serviceNameOperator": "=",
          "spanNameOperator": "=",
          "tags": []
        }
      },
      "pluginVersion": "11.5.2",
      "targets": [
        {
          "limit": 20,
          "query": "${traceId}",
          "queryType": "traceql",
          "refId": "A",
          "tableType": "traces"
        }
      ],
      "title": "Selected Trace (waterfall)",
      "type": "traces"
    },
    {
      "id": 108,
      "type": "table",
      "title": "Service graph edges (RPS)",
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 39 },
      "datasource": { "type": "prometheus", "uid": "${ds_prom}" },
      "pluginVersion": "11.5.2",
      "targets": [
        {
          "refId": "A",
          "expr": "sort_desc(sum(rate(traces_service_graph_request_total{client=~\"$client\", server=~\"$server\"}[$__rate_interval])) by (client, server))",
          "format": "table"
        }
      ],
      "fieldConfig": { "defaults": { "custom": { "align": "auto", "cellOptions": { "type": "auto" } } }, "overrides": [] },
      "options": { "showHeader": true, "cellHeight": "sm", "footer": { "show": false } }
    },
    {
      "id": 109,
      "type": "timeseries",
      "title": "Service graph edge p95 (client)",
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 39 },
      "datasource": { "type": "prometheus", "uid": "${ds_prom}" },
      "pluginVersion": "11.5.2",
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum(rate(traces_service_graph_request_client_seconds_bucket{client=~\"$client\", server=~\"$server\"}[$__rate_interval])) by (le, client, server))",
          "legendFormat": "{{client}} â†’ {{server}}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "options": { "legend": { "showLegend": true, "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "single" } }
    }
  ],
  "preload": false,
  "refresh": "30s",
  "schemaVersion": 40,
  "tags": ["tempo", "prometheus", "tracing", "spanmetrics", "servicegraph"],
  "templating": {
    "list": [
      {
        "name": "ds_tempo",
        "label": "Tempo datasource",
        "type": "datasource",
        "query": "tempo",
        "refresh": 1,
        "current": {},
        "options": []
      },
      {
        "name": "ds_prom",
        "label": "Prometheus datasource",
        "type": "datasource",
        "query": "prometheus",
        "refresh": 1,
        "current": {},
        "options": []
      },
      {
        "name": "service",
        "label": "Service (spanmetrics)",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${ds_prom}" },
        "refresh": 1,
        "query": { "query": "label_values(traces_spanmetrics_calls_total, service)", "refId": "PromVarService" },
        "includeAll": true,
        "allValue": ".*",
        "multi": true,
        "sort": 1,
        "current": {}
      },
      {
        "name": "client",
        "label": "Client (servicegraph)",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${ds_prom}" },
        "refresh": 1,
        "query": { "query": "label_values(traces_service_graph_request_total, client)", "refId": "PromVarClient" },
        "includeAll": true,
        "allValue": ".*",
        "multi": true,
        "sort": 1,
        "current": {}
      },
      {
        "name": "server",
        "label": "Server (servicegraph)",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${ds_prom}" },
        "refresh": 1,
        "query": { "query": "label_values(traces_service_graph_request_total, server)", "refId": "PromVarServer" },
        "includeAll": true,
        "allValue": ".*",
        "multi": true,
        "sort": 1,
        "current": {}
      },
      {
        "name": "traceql",
        "label": "TraceQL filter",
        "type": "textbox",
        "current": { "text": "{true}", "value": "{true}" }
      },
      {
        "name": "traceId",
        "label": "Trace ID",
        "type": "textbox",
        "current": { "text": "", "value": "" }
      }
    ]
  },
  "time": { "from": "now-6h", "to": "now" },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m"],
    "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d"]
  },
  "timezone": "browser",
  "title": "Tempo Traces + Span/Service Metrics",
  "uid": "tempo_traces_spanmetrics",
  "version": 1,
  "weekStart": ""
}
